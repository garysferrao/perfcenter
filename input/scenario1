variable
nusr 90
ccount 2
end

device
cpu
end

lan
lan1
end

host machine[2]
cpu count ccount
cpu buffer 99999
cpu schedP fcfs
end

task t1
cpu servt 0.01
end

task t2
cpu servt 0.1
end
 

task t3
cpu servt 0.01
end

server s1
thread count 10
thread buffer 99999
thread schedP fcfs
task t1
task t3
end


server s2
thread count 5
thread buffer 99999
thread schedP fcfs
task t2
end

deploy s1 machine[1]
deploy s2 machine[2]

deploy machine[1] lan1
deploy machine[2] lan1

scenario sc1 prob 1
    t1  t2 200 SYNC
    t2  t3 200
end


loadparams
   noofusers    nusr
   thinktime    exp(5)
end

modelparams
    method analytical
    type closed
    confint true
    replicationno 5
    noofrequests 200000
end

print "NumberofUsers	SC-1ResponseTime	s1-Throughput	s1-Utilization	s2-Throughput	s2-Utilization	Machine1CPU-Utilization	Machine2CPU-Utilization"
while(nusr < 151)
print nusr + "	"+ respt(sc1) + "	" + tput(machine[1]:s1) + "	" + util(machine[1]:s1) + "	" + tput(machine[2]:s2) + "	" + util(machine[2]:s2) + "	" + util(machine[1]:cpu) + "	" + util(machine[2]:cpu)
nusr = nusr + 10
end
